{
  "name": "gu-decks-assistant",
  "version": "0.2.10",
  "description": "this is Gods Unchained decks assistant app",
  "main": "index.js",
  "scripts": {
    "start": "npx electron-forge start",
    "package": "npx electron-forge package",
    "make": "npx electron-forge make",
    "format-code": "npx prettier --write .",
    "publish": "npx electron-forge publish"
  },
  "keywords": [],
  "author": "bugb",
  "license": "ISC",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "electron": "^15.1.1",
    "prettier": "2.4.1"
  },
  "dependencies": {
    "@electron-forge/publisher-github": "^6.0.0-beta.61",
    "axios": "^0.22.0",
    "axios-retry": "^3.2.0",
    "chokidar": "^3.5.2",
    "electron-ipc-socket": "^3.0.0",
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "fs-extra": "^8.1.0",
    "node-cache": "^5.1.2"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "setupExe": "gu-decks-assistant.exe",
            "name": "gu-decks-assistant"
          }
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "p2etools",
              "name": "gu-decks-assistant"
            },
            "draft": false
          }
        }
      ]
    }
  },
  "build": {
    "publish": [
      {
        "provider": "github",
        "owner": "p2etools",
        "repo": "gu-decks-assistant"
      }
    ]
  }
}
